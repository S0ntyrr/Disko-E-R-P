@model Proveedor
<h2>Detalle Proveedor</h2>
<p><strong>Nombre:</strong> @Model.Nombre</p>
<p><strong>Email:</strong> @Model.Email</p>
<p><strong>Teléfono:</strong> @Model.Telefono</p>
<p><strong>Dirección:</strong> @Model.Direccion</p>
<p><strong>Activo:</strong> @Model.Activo</p>
<a href="@Url.Action("Edit", new { id = Model.ProveedorId })">Editar</a>